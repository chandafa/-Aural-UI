{
  "name": "auralix-ui",
  "version": "1.2.0",
  "description": "Beautiful, accessible React UI components with Tailwind CSS, dark mode, and smooth GSAP/Framer Motion animations. 87+ production-ready components.",
  "author": "Auralix Team <auralix@example.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "react-components",
    "ui",
    "ui-components",
    "ui-library",
    "component-library",
    "tailwindcss",
    "tailwind",
    "glass-morphism",
    "glassmorphism",
    "typescript",
    "nextjs",
    "next",
    "design-system",
    "framer-motion",
    "gsap",
    "animation",
    "dark-mode",
    "accessible",
    "wcag",
    "button",
    "modal",
    "card",
    "tooltip"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chandafa/auralix-ui.git"
  },
  "homepage": "https://auralix-ui.netlify.app",
  "bugs": {
    "url": "https://github.com/chandafa/auralix-ui/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "bin": {
    "auralix-ui": "./dist/bin.js"
  },
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "src"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0 || ^19.0.0",
    "react-dom": ">=18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.3",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "next": "^14.1.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "@types/three": "^0.160.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "clsx": "^2.1.0",
    "commander": "^14.0.2",
    "fs-extra": "^11.3.3",
    "lucide-react": "^0.468.0",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "tailwind-merge": "^2.2.0",
    "three": "^0.160.0"
  }
}
